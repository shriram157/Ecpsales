<mvc:View controllerName="zecp.controller.App" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns="sap.m" xmlns:tnt="sap.tnt">
	<App >
		<Page >
			<customHeader>
				<Bar class="sapUiSmallPaddingBeginEnd clCustomHeaderBg">
					<contentLeft>
						<Image id="idLexusLogo" src="images/Lexus_ENV1.png" width="100px"/>
						<Image id="idToyotaLogo" src="images/toyota_logo.png" width="100px"/>
						<Text text="|" class="sapUiTinyMarginBeginEnd"/>
						<Text class="clCustomFont" text="{i18n>appTitle}"/>
					</contentLeft>
				</Bar>
			</customHeader>
			<App id="idAppControl"/>
			<footer>
				<OverflowToolbar>
					<ToolbarSpacer/>
				</OverflowToolbar>
			</footer>
		</Page>
	</App>
</mvc:View>