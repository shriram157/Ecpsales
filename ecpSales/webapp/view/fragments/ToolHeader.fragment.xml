	<tnt:ToolHeader>
		<Button text="{i18n>NewApplication}" id="idNewApp" press="handleLinkPress" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData priority="High"/>
			</layoutData>
		</Button>
		<Button text="{i18n>ViewUpdateApp}" id="idViewUpdate" press="handleLinkPress" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData priority="High"/>
			</layoutData>
		</Button>
		<Button text="{i18n>AgreementInquiry}" id="idAgrInq" press="handleLinkPress" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData priority="High"/>
			</layoutData>
		</Button>
		<tnt:ToolHeaderUtilitySeparator/>
	</tnt:ToolHeader>
</core:FragmentDefinition>