												<Label text="{i18n>Email}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/EmailAddress}"/>
												<Label text="{i18n>Home}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/PhoneNumber}"/>
												<Label text="{i18n>Business}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/FaxNumber}"/>
											</f:content>
										</f:SimpleForm>
									</html:div>
									<html:div class="exampleDiv">
										<f:SimpleForm editable="true" layout="ResponsiveGridLayout" title="{i18n>VehicleOwner}" labelSpanXL="5" labelSpanL="5" labelSpanM="5"
											labelSpanS="4" adjustLabelSpan="true" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
											singleContainerFullSize="false">
											<f:content>
											
												<Label text="{i18n>Customer}"/>
												<Text text="{path: 'LocalDataModel>/BusinessPartnerData/BusinessPartner'}"/>
												<Label text="{i18n>Type}"/>
												<Text text="{path: 'LocalDataModel>/PricingModelData/BPTYPE', formatter : 'zecp.utils.formatter.fnFormatType'}"/>
												<Label text="{i18n>Language}"/>
												<Text text="{path: 'LocalDataModel>/BusinessPartnerData/Language'}"/>
												<Label text="{i18n>Name}"/>
												<Text text="{LocalDataModel>/BusinessPartnerName/LastName} {LocalDataModel>/BusinessPartnerName/FirstName}"/>
												<Label text="{i18n>Address}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/StreetName}"/>
												<Label text="{i18n>City}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/CityName}"/>
												<Label text="{i18n>Province}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/Region}"/>
												<Label text="{i18n>PostalCode}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/PostalCode}"/>
												<Label text="{i18n>Email}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/EmailAddress}"/>
												<Label text="{i18n>Home}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/PhoneNumber}"/>
												<Label text="{i18n>Business}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/FaxNumber}"/>
												<Label text="{i18n>Mobile}"/>
												<Text text="{LocalDataModel>/BusinessPartnerData/MobileNumber}"/>
											</f:content>
										</f:SimpleForm>
									</html:div>
									<html:div class="exampleDiv">
										<f:SimpleForm editable="true" layout="ResponsiveGridLayout" title="{i18n>VehicleDetails}" labelSpanXL="5" labelSpanL="5" labelSpanM="5"
											labelSpanS="4" adjustLabelSpan="true" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
											singleContainerFullSize="false">
											<f:content>
												<core:Title text="VIN : {LocalDataModel>/AgreementInfo/VIN}"/>
												<Label text="{i18n>Make}"/>
												<Text text="{LocalDataModel>/PricingModelData/MAKE}"/>
												<Label text="{i18n>Model}"/>
												<Text text="{LocalDataModel>/PricingModelData/VEHICLEMODEL}"/>
												<Label text="{i18n>Year}"/>
												<Text text="{LocalDataModel>/PricingModelData/ZZMOYR}"/>
												<!--<Label text="{i18n>AutoCode}"/>-->
												<!--<Text text="{LocalDataModel>/PricingModelData/ZECP_DEALCODE}"/>-->
												<Label text="{i18n>RegDate}"/>
												<Text
													text="{path:'LocalDataModel>/PricingModelData/REG_DATE', type: 'sap.ui.model.type.Date', constraints: {displayFormat: 'Date' }, formatOptions: { style: 'medium', strictParsing: true , UTC: true}}"/>
												<!--<Label text="{i18n>SaleType}"/>-->
												<!--<Text text="{LocalDataModel>/PricingModelData/ZECP_SALETYPE}"/>-->
											</f:content>
										</f:SimpleForm>
